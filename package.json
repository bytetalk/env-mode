{
  "name": "set-node-env",
  "version": "0.0.8",
  "description": "Set node env from .env and .env.mode",
  "main": "src/index.js",
  "bin": {
    "set-node-env": "bin/cli.js"
  },
  "scripts": {
    "lint": "eslint --fix bin src test require.js",
    "test:inline": "node test/inline",
    "test:require:development": "node -r ./require test/require --env-mode=development --env-debug",
    "test:require:test": "node -r ./require test/require --env-mode=test --env-debug",
    "test:require:production": "node -r ./require test/require --env-mode=production --env-debug",
    "test:cli:development": "node bin/cli --mode=development --debug node test/cli",
    "test:cli:test": "node bin/cli --mode=test --debug node test/cli",
    "test:cli:production": "node bin/cli --mode=production --debug node test/cli"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytetalk/set-node-env.git"
  },
  "keywords": [
    "set-node-env",
    ".env",
    ".env.development",
    ".env.test",
    ".env.production",
    ".env.mode",
    "process.env",
    "environment",
    "env",
    "node",
    "set"
  ],
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0"
  },
  "dependencies": {}
}
